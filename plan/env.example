# Discord Bot Token from Discord Developer Portal
DISCORD_TOKEN=your_discord_bot_token_here

# Binance API Keys (Use testnet keys for safety)
BINANCE_API_KEY=your_binance_api_key
BINANCE_SECRET=your_binance_secret_key

# Environment
ENVIRONMENT=development
LOG_LEVEL=INFO

# Database (Optional - for advanced features)
DATABASE_URL=postgresql://user:password@localhost/trading_bot

# Redis (Optional - for caching)
REDIS_URL=redis://localhost:6379

# Security
ENCRYPTION_KEY=your_32_byte_encryption_key_here

# Alerts (Optional)
WEBHOOK_URL=your_discord_webhook_url_for_alerts
